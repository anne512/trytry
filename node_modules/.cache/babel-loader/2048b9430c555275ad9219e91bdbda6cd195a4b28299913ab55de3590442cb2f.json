{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      products: [{\n        id: 1,\n        name: 'Black Coffee',\n        price: 39\n      }, {\n        id: 2,\n        name: 'Espresso',\n        price: 49\n      }, {\n        id: 3,\n        name: 'Iced Latte',\n        price: 59\n      }, {\n        id: 4,\n        name: 'Americano',\n        price: 69\n      }, {\n        id: 5,\n        name: 'Cappuccino',\n        price: 79\n      }, {\n        id: 6,\n        name: 'Mocha',\n        price: 89\n      }, {\n        id: 7,\n        name: 'Caramel Macchiato',\n        price: 99\n      }, {\n        id: 8,\n        name: 'Matcha',\n        price: 109\n      }, {\n        id: 9,\n        name: 'Cold Brew',\n        price: 119\n      }, {\n        id: 10,\n        name: 'Frappe',\n        price: 129\n      }],\n      cart: []\n    };\n  },\n  computed: {\n    totalPrice() {\n      return this.cart.reduce((total, item) => {\n        return total + item.product.price * item.quantity;\n      }, 0);\n    },\n    menuRows() {\n      const rows = [];\n      for (let i = 0; i < this.products.length; i += 3) {\n        rows.push(this.products.slice(i, i + 3));\n      }\n      return rows;\n    }\n  },\n  methods: {\n    addToCart(product) {\n      const found = this.cart.find(item => item.product.id === product.id);\n      if (found) {\n        found.quantity++;\n      } else {\n        this.cart.push({\n          product: product,\n          quantity: 1\n        });\n      }\n    },\n    removeFromCart(index) {\n      this.cart.splice(index, 1);\n    },\n    updateQuantity(index) {\n      const quantity = parseInt(this.cart[index].quantity);\n      if (quantity <= 0) {\n        this.cart.splice(index, 1);\n      }\n    },\n    incrementQuantity(index) {\n      this.cart[index].quantity++;\n    },\n    decrementQuantity(index) {\n      if (this.cart[index].quantity > 1) {\n        this.cart[index].quantity--;\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","products","id","name","price","cart","computed","totalPrice","reduce","total","item","product","quantity","menuRows","rows","i","length","push","slice","methods","addToCart","found","find","removeFromCart","index","splice","updateQuantity","parseInt","incrementQuantity","decrementQuantity"],"sources":["C:\\Users\\User\\Tena\\try\\src\\components\\ShopCart.vue"],"sourcesContent":["<template>\r\n    <div class=\"container\">\r\n      <!-- Product Menu -->\r\n      <div class=\"menu\">\r\n        <h2>Coffee Menu List</h2>\r\n        <div class=\"product-list\">\r\n          <div class=\"row\" v-for=\"(row, index) in menuRows\" :key=\"index\">\r\n            <div v-for=\"product in row\" :key=\"product.id\" class=\"product card\">\r\n              <div class=\"card-body\">\r\n                <div class=\"product-info\">\r\n                  <p>{{ product.name }} - ₱{{ product.price }}</p>\r\n                  <hr class=\"break-line\">\r\n                  <button @click=\"addToCart(product)\" class=\"add-to-cart-button\">Add to Cart</button>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n  \r\n      <!-- Shopping Cart -->\r\n      <div class=\"cart\">\r\n        <h2>Your Cart</h2>\r\n        <div v-if=\"cart.length === 0\" class=\"empty-cart-message\">Your Cart is Empty!</div>\r\n        <div v-else>\r\n          <div v-for=\"(item, index) in cart\" :key=\"index\" class=\"cart-item\">\r\n            <div class=\"cart-info\">\r\n              <p>{{ item.product.name }} - ₱{{ item.product.price }}</p>\r\n              <div class=\"quantity-controls\">\r\n                <button @click=\"decrementQuantity(index)\" class=\"quantity-button\">-</button>\r\n                <input type=\"number\" v-model=\"item.quantity\" min=\"1\" class=\"quantity-input\" @input=\"updateQuantity(index)\">\r\n                <button @click=\"incrementQuantity(index)\" class=\"quantity-button\">+</button>\r\n              </div>\r\n              <button class=\"remove-button\" @click=\"removeFromCart(index)\">Remove</button>\r\n            </div>\r\n          </div>\r\n          <p>Total: <span class=\"total-price\">₱{{ totalPrice }}</span></p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  \r\n  <script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        products: [\r\n          { id: 1, name: 'Black Coffee', price: 39 },\r\n          { id: 2, name: 'Espresso', price: 49 },\r\n          { id: 3, name: 'Iced Latte', price: 59 },\r\n          { id: 4, name: 'Americano', price: 69 },\r\n          { id: 5, name: 'Cappuccino', price: 79 },\r\n          { id: 6, name: 'Mocha', price: 89 },\r\n          { id: 7, name: 'Caramel Macchiato', price: 99 },\r\n          { id: 8, name: 'Matcha', price: 109 },\r\n          { id: 9, name: 'Cold Brew', price: 119 },\r\n          { id: 10, name: 'Frappe', price: 129 },\r\n        ],\r\n        cart: [],\r\n      };\r\n    },\r\n    computed: {\r\n      totalPrice() {\r\n        return this.cart.reduce((total, item) => {\r\n          return total + (item.product.price * item.quantity);\r\n        }, 0);\r\n      },\r\n      menuRows() {\r\n        const rows = [];\r\n        for (let i = 0; i < this.products.length; i += 3) {\r\n          rows.push(this.products.slice(i, i + 3));\r\n        }\r\n        return rows;\r\n      }\r\n    },\r\n    methods: {\r\n      addToCart(product) {\r\n        const found = this.cart.find(item => item.product.id === product.id);\r\n        if (found) {\r\n          found.quantity++;\r\n        } else {\r\n          this.cart.push({ product: product, quantity: 1 });\r\n        }\r\n      },\r\n      removeFromCart(index) {\r\n        this.cart.splice(index, 1);\r\n      },\r\n      updateQuantity(index) {\r\n        const quantity = parseInt(this.cart[index].quantity);\r\n        if (quantity <= 0) {\r\n          this.cart.splice(index, 1);\r\n        }\r\n      },\r\n      incrementQuantity(index) {\r\n        this.cart[index].quantity++;\r\n      },\r\n      decrementQuantity(index) {\r\n        if (this.cart[index].quantity > 1) {\r\n          this.cart[index].quantity--;\r\n        }\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style>\r\n  .container {\r\n    display: flex;\r\n  }\r\n  \r\n  .menu {\r\n    flex: 1;\r\n    margin-right: 20px;\r\n  }\r\n  \r\n  .cart {\r\n    flex: 1;\r\n    margin-left: 20px;\r\n  }\r\n  \r\n  .row {\r\n    display: flex;\r\n  }\r\n  \r\n  .product {\r\n    flex: 0 0 calc(33.33% - 20px);\r\n    margin: 10px;\r\n  }\r\n  \r\n  .card {\r\n    border: 1px solid black;\r\n    border-radius: 5px;\r\n  }\r\n  \r\n  .card-body {\r\n    padding: 10px;\r\n  }\r\n  \r\n  .product-info {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n  }\r\n  \r\n  .add-to-cart-button {\r\n    background-color: green;\r\n    color: white;\r\n    border: none;\r\n    padding: 8px 16px;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  .add-to-cart-button:hover {\r\n    background-color: darkgreen;\r\n  }\r\n  \r\n  .cart-item {\r\n    margin-bottom: 10px;\r\n  }\r\n  \r\n  .cart-info {\r\n    display: flex;\r\n    align-items: center;\r\n    margin-right: 10px; /* Add margin-right to create space */\r\n  }\r\n  \r\n  .quantity-controls {\r\n    display: flex;\r\n    align-items: center;\r\n    margin-left: 10px; /* Add margin-left to create space */\r\n  }\r\n  \r\n  .quantity-button {\r\n    color: black;\r\n    border: 1px solid LightGray;\r\n    border-radius: 5px;\r\n    padding: 5px 10px;\r\n    cursor: pointer;\r\n    background-color: White;\r\n  }\r\n  \r\n  .quantity-input {\r\n    width: 50px;\r\n    text-align: center;\r\n    border: 1px solid LightGray; /* Represent #ccc in CSS color name */\r\n    border-radius: 5px; /* Add border-radius for rounded corners */\r\n    padding: 5px;\r\n    box-sizing: border-box; /* Ensure padding and border are included in the width */\r\n  }\r\n  \r\n  .quantity-input::-webkit-inner-spin-button,\r\n  .quantity-input::-webkit-outer-spin-button {\r\n    -webkit-appearance: none;\r\n    margin: 0;\r\n  }\r\n  \r\n  .quantity-input::-moz-outer-spinbutton {\r\n    appearance: none;\r\n    margin: 0;\r\n  }\r\n  \r\n  .remove-button {\r\n    background-color: red;\r\n    color: white;\r\n    border: none;\r\n    padding: 8px 16px;\r\n    cursor: pointer;\r\n    margin-left: 10px; /* Add margin-left to create space */\r\n  }\r\n  \r\n  .remove-button:hover {\r\n    background-color: darkred;\r\n  }\r\n  \r\n  .empty-cart-message {\r\n    color: red;\r\n  }\r\n  \r\n  .total-price {\r\n    font-weight: bold;\r\n  }\r\n  </style>\r\n  "],"mappings":";AA4CE,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,CACR;QAAEC,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,cAAc;QAAEC,KAAK,EAAE;MAAG,CAAC,EAC1C;QAAEF,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,UAAU;QAAEC,KAAK,EAAE;MAAG,CAAC,EACtC;QAAEF,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,YAAY;QAAEC,KAAK,EAAE;MAAG,CAAC,EACxC;QAAEF,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,WAAW;QAAEC,KAAK,EAAE;MAAG,CAAC,EACvC;QAAEF,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,YAAY;QAAEC,KAAK,EAAE;MAAG,CAAC,EACxC;QAAEF,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,OAAO;QAAEC,KAAK,EAAE;MAAG,CAAC,EACnC;QAAEF,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,mBAAmB;QAAEC,KAAK,EAAE;MAAG,CAAC,EAC/C;QAAEF,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,QAAQ;QAAEC,KAAK,EAAE;MAAI,CAAC,EACrC;QAAEF,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,WAAW;QAAEC,KAAK,EAAE;MAAI,CAAC,EACxC;QAAEF,EAAE,EAAE,EAAE;QAAEC,IAAI,EAAE,QAAQ;QAAEC,KAAK,EAAE;MAAI,CAAC,CACvC;MACDC,IAAI,EAAE;IACR,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,UAAUA,CAAA,EAAG;MACX,OAAO,IAAI,CAACF,IAAI,CAACG,MAAM,CAAC,CAACC,KAAK,EAAEC,IAAI,KAAK;QACvC,OAAOD,KAAI,GAAKC,IAAI,CAACC,OAAO,CAACP,KAAI,GAAIM,IAAI,CAACE,QAAS;MACrD,CAAC,EAAE,CAAC,CAAC;IACP,CAAC;IACDC,QAAQA,CAAA,EAAG;MACT,MAAMC,IAAG,GAAI,EAAE;MACf,KAAK,IAAIC,CAAA,GAAI,CAAC,EAAEA,CAAA,GAAI,IAAI,CAACd,QAAQ,CAACe,MAAM,EAAED,CAAA,IAAK,CAAC,EAAE;QAChDD,IAAI,CA